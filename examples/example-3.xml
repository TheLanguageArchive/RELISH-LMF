    <lmf:GlobalInformation>
        <tei:f name="languageCoding" fVal="ISO 639-3"/>
    </lmf:GlobalInformation>
    <lmf:Lexicon>
        <tei:f name="language" fVal="eng"/>
        <lmf:LexicalEntry>
            <tei:f name="partOfSpeech" fVal="commonNoun"/>
            <lmf:Lemma type="Form">
                <tei:f name="writtenForm" fVal="clergyman"/>
