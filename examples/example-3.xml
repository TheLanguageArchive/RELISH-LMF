<lmf:LexicalResource xmlns:lmf="http://www.lexicalmarkupframework.org/" xmlns:tei="http://www.tei-c.org/ns/1.0"  xmlns:dcr="http://www.isocat.org/ns/dcr" lmfVersion="ISO 24613:2008">
    <tei:fsdDecl>
        <tei:fsDecl type="lmf:GlobalInformation">
            <tei:fDecl name="languageCoding" dcr:datcat="http://www.isocat.org/datcat/DC-2008">
                <tei:vRange>
                    <tei:vNot>
                        <tei:string/>
                    </tei:vNot>
                </tei:vRange>
            </tei:fDecl>
        </tei:fsDecl>
        <tei:fsDecl type="lmf:Lexicon">
            <tei:fDecl name="language" dcr:datcat="http://www.isocat.org/datcat/DC-1969">
                <tei:vRange>
                    <tei:vNot>
                        <tei:string/>
                    </tei:vNot>
                </tei:vRange>
            </tei:fDecl>
        </tei:fsDecl>
        <tei:fsDecl type="lmf:LexicalEntry">
