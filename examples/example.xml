        <feat att="language" val="eng" dcr:datcat="http://www.isocat.org/datcat/DC-1969"/>
        <LexicalEntry xml:id="le0001">
            <feat att="partOfSpeech" dcr:datcat="http://www.isocat.org/datcat/DC-1345"
